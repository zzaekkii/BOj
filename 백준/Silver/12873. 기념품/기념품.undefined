#include <bits/stdc++.h>
using namespace std;

int N;
vector<int> v;

int main(){
    ios::sync_with_stdio(0); cin.tie(0);
    cin >> N;
    
    for(int i=1;i<=N;i++)
    	v.push_back(i);
    
    int cur=0;
    for(int i=1;i<N;i++){
    	long long n=1LL*i*i*i;
    	n=(n-1)%v.size();
    	
    	cur=(cur+n)%v.size();
    	v.erase(v.begin()+cur);
	}
	
	cout << v[0];
}