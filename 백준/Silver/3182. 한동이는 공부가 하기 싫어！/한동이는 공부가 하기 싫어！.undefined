#include <bits/stdc++.h>
using namespace std;

int N, T;
int nxt[1001];

int main(){
	ios_base::sync_with_stdio(0); cin.tie(0);
	cin >> N;
	
	for(int i=1;i<=N;i++)
		cin >> nxt[i];
	
	int ans=0, mx=0;
	for(int i=1;i<=N;i++){
		int cnt=0, t=i;
		bool tmp[1001]={0};
		
		while(!tmp[t]){
			tmp[t]=1;
			t=nxt[t];
			cnt++;
		}
		
		if(cnt>mx){
			ans=i;
			mx=cnt;
		}
	}
	
	cout << ans;
}