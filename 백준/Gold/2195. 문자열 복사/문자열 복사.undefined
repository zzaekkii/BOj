#include <bits/stdc++.h>
using namespace std;

string S, P;

int main(){
	ios_base::sync_with_stdio(0); cin.tie(0);
	cin >> S >> P;
	
	int s_len=S.length(), p_len=P.length();
	int idx=0, total=0;
	
	while(idx<p_len){
		int len=0;
		
		for(int j=0;j<s_len;j++){
			int k=0;
			while((idx+k<p_len)&&(j+k<s_len)&&P[idx+k]==S[j+k])k++;
			
			len=max(len,k);
		}
		
		idx+=len;
		total++;
	}
	
	cout << total;
}