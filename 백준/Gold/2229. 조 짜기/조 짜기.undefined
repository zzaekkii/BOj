#include <bits/stdc++.h>
using namespace std;

int N;
int score[1001];
int dp[1001];

int main(){
	ios_base::sync_with_stdio(0); cin.tie(0);
	cin >> N;
	
	for(int i=0;i<N;i++)
		cin >> score[i];
	
	for(int i=0;i<N;i++){
		int mx,mn;
		mx=mn=score[i];
		
		for(int j=i;j>=0;j--){
			mx=max(mx,score[j]);
			mn=min(mn,score[j]);
			
			dp[i+1]=max(dp[i+1],dp[j]+mx-mn);
		}
	}
	
	cout << dp[N];
}